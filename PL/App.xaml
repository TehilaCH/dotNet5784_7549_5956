<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             xmlns:converters="clr-namespace:PL.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:EngineersCollection x:Key="EngineersCollectionKey" />
        <local:StatusCollection x:Key="StatusCollectionKey" /> 
        <converters:ConvertIdToContent x:Key="ConvertIdToContentKey"/>
        <converters:EditModeToBoolConverter x:Key="EditModeConverterKey"/>
        <converters:BoolConverter x:Key="BoolConverterKey"/>
        <converters:ConvertTimeSpan x:Key="ConvertTimeSpanKey"/>
        
    </Application.Resources>
</Application>